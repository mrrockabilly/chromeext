{ 
	"manifest_version": 2, 
	"name": "Live/Work", 
	"description": "Keep you productive...or not", 
	"version": "0.0.1" ,
	"background" : {
		"scripts" : ["js/jquery-3.1.0.min.js"]
		}, 
	"content_scripts":[
		{
			"matches": ["*://www.facebook.com/*"], 
			"js":["js/jquery-3.1.0.min.js", "js/values.js", "js/prompt.js", "js/work.js"]
		},
		{
			"matches": ["*://twitter.com/*"], 
			"js":["js/jquery-3.1.0.min.js","js/values.js", "js/prompt.js", "js/work.js"]
		},
		{
			"matches": ["*://instagram.com/*"], 
			"js":["js/jquery-3.1.0.min.js","js/values.js", "js/prompt.js", "js/work.js"]
		},
		{
			"matches": ["*://stackoverflow.com/*"], 
			"js":["js/jquery-3.1.0.min.js","js/values.js", "js/prompt.js", "js/home.js"]
		},
		{
			"matches": ["*://api.jquery.com/*"], 
			"js":["js/jquery-3.1.0.min.js","js/values.js", "js/prompt.js", "js/home.js"]
		},
		{
			"matches": ["*://developer.mozilla.org/*"], 
			"js":["js/jquery-3.1.0.min.js","js/values.js","js/prompt.js", "js/home.js"]
		}

]}